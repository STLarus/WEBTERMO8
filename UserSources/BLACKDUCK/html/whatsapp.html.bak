      //idcredit=setInterval("loadCredit()", 2000);
      $(document).ready(function () {
        $.ajax({
          type: "GET",
          url: "wapp.xml?sid=" + Math.random(),
          dataType: "xml",
          cache: false,
          success: function (xml) {
